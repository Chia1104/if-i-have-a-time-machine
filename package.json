{
  "name": "if-i-have-a-time-machine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "migration:init": "prisma migrate dev --name init --preview-feature",
    "migration:save": "prisma migrate dev --name --preview-feature",
    "db:push": "prisma db push --preview-feature",
    "prisma:studio": "prisma studio",
    "analyze": "cross-env ANALYZE=true npm run build",
    "type:check": "tsc --noEmit"
  },
